{
  "name": "postcss-font-grabber",
  "version": "1.0.6",
  "description": "Grab remote font in @font-face, download it and update your CSS, end of story.",
  "author": "AaronJan <aaronjan@qq.com>",
  "homepage": "https://github.com/AaronJan/postcss-font-grabber",
  "repository": {
    "type": "git",
    "url": "https://github.com/AaronJan/postcss-font-grabber.git"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=0.11"
  },
  "scripts": {
    "test": "nyc ava",
    "build": "babel src/ --out-dir dist/ --source-maps inline"
  },
  "keywords": [
    "postcss-plugin",
    "css",
    "font-face",
    "font"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "nyc": "^6.4.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "sourceMaps": "inline"
  },
  "dependencies": {
    "lodash": "^4.13.0",
    "postcss": "^5.0.21"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
